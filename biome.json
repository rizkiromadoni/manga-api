{
	"$schema": "https://biomejs.dev/schemas/1.8.1/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"complexity": {
				"useRegexLiterals": "off"
			}
		}
	},
	"javascript": {
		"formatter": {
			"trailingCommas": "none",
			"semicolons": "asNeeded",
			"quoteStyle": "double"
		}
	}
}
